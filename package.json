{
  "name": "express-o",
  "repository": "https://github.com/positlabs/express-o",
  "version": "2.4.0",
  "license": "MIT",
  "private": false,
  "scripts": {
    "start": "node ./bin/www",
    "start:local": "pm2 start ./dev/env/local.yaml && pm2 logs",
    "localtunnel": "lt --port 3000",
    "info:repo": "basename -s .git `git config --get remote.origin.url`",
    "info:branch": "git rev-parse --abbrev-ref HEAD",
    "info:tag": "echo $(npm run -s info:repo)-$(npm run -s info:branch)"
  },
  "dependencies": {
    "babel-preset-env": "^1.6.0",
    "babelify": "^7.3.0",
    "body-parser": "^1.13.3",
    "browserify-middleware": "^7.1.0",
    "cookie-parser": "^1.3.5",
    "express": "^4.14.0",
    "inject-scss": "^1.0.0",
    "jquery": "^3.1.1",
    "model-o": "^1.0.1",
    "node-sass-middleware": "^0.10.0",
    "pug": "^2.0.0-beta6",
    "request": "^2.78.0",
    "shrink-ray": "^0.1.3",
    "x-tag": "^1.5.11"
  },
  "engines": {
    "node": "8.4.0"
  },
  "devDependencies": {
    "livereload": "^0.6.2",
    "node-notifier": "^4.6.1"
  }
}
